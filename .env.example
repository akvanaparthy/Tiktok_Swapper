# Airtable Configuration
AIRTABLE_TOKEN=your_airtable_token
AIRTABLE_BASE_ID=your_base_id

# Apify Token (for TikTok/Instagram scraping)
APIFY_TOKEN=your_apify_token

# FAL.ai API Keys (you can add multiple - FAL_API_KEY_1, FAL_API_KEY_2, etc.)
FAL_API_KEY_1=your_fal_key_1
# FAL_API_KEY_2=your_fal_key_2
# FAL_API_KEY_3=your_fal_key_3

# Wavespeed API Keys (you can add multiple - WAVESPEED_API_KEY_1, WAVESPEED_API_KEY_2, etc.)
WAVESPEED_API_KEY_1=your_wavespeed_key_1
# WAVESPEED_API_KEY_2=your_wavespeed_key_2
# WAVESPEED_API_KEY_3=your_wavespeed_key_3

# API Rotation Settings
# How many requests per API key before rotating (default: 1 = alternate every request)
# Set to 0 for round-robin (automatic alternation)
# Set to specific number to use each API that many times before rotating
FAL_REQUESTS_PER_KEY=1
WAVESPEED_REQUESTS_PER_KEY=1

# Logging
LOG_LEVEL=info

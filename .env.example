# Airtable Configuration
AIRTABLE_TOKEN=your_airtable_token
AIRTABLE_BASE_ID=your_base_id

# Apify Token (for TikTok/Instagram scraping)
APIFY_TOKEN=your_apify_token

# FAL.ai API Keys for IMAGE generation (add multiple for rotation)
FAL_IMAGE_API_KEY_1=your_fal_image_key_1
# FAL_IMAGE_API_KEY_2=your_fal_image_key_2
# FAL_IMAGE_API_KEY_3=your_fal_image_key_3

# FAL.ai API Keys for VIDEO generation (add multiple for rotation)
FAL_VIDEO_API_KEY_1=your_fal_video_key_1
# FAL_VIDEO_API_KEY_2=your_fal_video_key_2
# FAL_VIDEO_API_KEY_3=your_fal_video_key_3

# Wavespeed API Keys for IMAGE generation (add multiple for rotation)
WAVESPEED_IMAGE_API_KEY_1=your_wavespeed_image_key_1
# WAVESPEED_IMAGE_API_KEY_2=your_wavespeed_image_key_2
# WAVESPEED_IMAGE_API_KEY_3=your_wavespeed_image_key_3

# Wavespeed API Keys for VIDEO generation (add multiple for rotation)
WAVESPEED_VIDEO_API_KEY_1=your_wavespeed_video_key_1
# WAVESPEED_VIDEO_API_KEY_2=your_wavespeed_video_key_2
# WAVESPEED_VIDEO_API_KEY_3=your_wavespeed_video_key_3

# API Rotation Settings
# How many requests per API key before rotating (default: 1 = alternate every request)
FAL_IMAGE_REQUESTS_PER_KEY=1
FAL_VIDEO_REQUESTS_PER_KEY=1
WAVESPEED_IMAGE_REQUESTS_PER_KEY=1
WAVESPEED_VIDEO_REQUESTS_PER_KEY=1

# Logging
LOG_LEVEL=info
